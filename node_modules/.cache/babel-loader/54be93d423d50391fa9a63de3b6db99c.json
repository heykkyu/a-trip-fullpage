CAAA,SAAA,yBAAA,gBAAA,WANA,CAOA;;AACA,MAAA,QAAA,CAAA,aAAA,CAAA,WAAA,EAAA,WAAA,CAAA,OAAA,EARA,CASA;;AACA,UAAA,eAAA,GAAA,QAAA,CAAA,aAAA,CAAA,IAAA,CAAA;AACA,MAAA,eAAA,CAAA,YAAA,CAAA,iBAAA,EAAA,SAAA,gBAAA;AACA,MAAA,eAAA,CAAA,SAAA,4BAAA,gBAAA,uBAAA,gBAAA,UAZA,CAaA;;AACA,UAAA,iBAAA,GAAA,QAAA,CAAA,aAAA,CAAA,OAAA,CAAA;AACA,MAAA,iBAAA,CAAA,WAAA,CAAA,eAAA,EAfA,CAgBA;;AACA,WAAA,OAAA,CAAA,OAAA,CAAA,IAAA,eAAA,gBAAA,GAjBA,CAkBA;AACA;AACA;AACA;;AACA,WAAA,KAAA,CAAA,QAAA,CAAA,KAAA;AACA,KA5BA;AA6BA,IAAA,aA7BA,2BA6BA;AACA,UAAA,QAAA,GAAA,QAAA,CAAA,aAAA,CAAA,WAAA,EAAA,gBAAA,CAAA,aAAA,CAAA;AACA,UAAA,WAAA,GAAA,QAAA,CAAA,QAAA,CAAA,MAAA,GAAA,CAAA,CAAA,CAFA,CAGA;;AACA,MAAA,WAAA,CAAA,UAAA,CAAA,WAAA,CAAA,WAAA,EAJA,CAKA;;AACA,WAAA,OAAA,CAAA,OAAA,CAAA,GAAA,GANA,CAOA;;AACA,UAAA,iBAAA,GAAA,QAAA,CAAA,gBAAA,CAAA,UAAA,CAAA;AACA,UAAA,QAAA,GAAA,iBAAA,CAAA,iBAAA,CAAA,MAAA,GAAA,CAAA,CAAA;AACA,MAAA,QAAA,CAAA,UAAA,CAAA,WAAA,CAAA,QAAA;AACA,KAxCA;AAyCA,IAAA,gBAzCA,8BAyCA;AACA,WAAA,OAAA,CAAA,UAAA,GAAA,CAAA,KAAA,OAAA,CAAA,UAAA;AACA,KA3CA;AA4CA,IAAA,eA5CA,6BA4CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,uBAAA;AACA,WAAA,OAAA,CAAA,SAAA,GAAA,CAAA,KAAA,OAAA,CAAA,SAAA;AACA;AA/CA;AArBA,CAAA","sourcesContent":["<template>\n  <div id=\"app\">\n      <trip-head\n        :anchors=\"options.anchors\"\n        :page=\"page\"\n      >\n      </trip-head>\n      \n      <full-page ref=\"fullpage\" :options=\"options\" id=\"fullpage\">\n          <div class=\"section\">\n            <trip-page1\n\n            ></trip-page1>\n          </div>\n          <div class=\"section\">\n              <div class=\"slide\">\n                  <h3>Slide 2.1</h3>\n              </div>\n              <div class=\"slide\">\n                  <h3>Slide 2.2</h3>\n              </div>\n              <div class=\"slide\">\n                  <h3>Slide 2.3</h3>\n              </div>\n          </div>\n          <div class=\"section\">\n              <h3>Section 3</h3>\n          </div>\n      </full-page>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'app',\n    components: {\n      TripHead: require('./components/pages/Header.vue').default,\n      TripPage1: require('./components/pages/Page1.vue').default,\n    },\n    data () {\n      return {\n        page: 'page1',\n        options: {\n          licenseKey: 'YOUR_KEY_HERE',\n          afterLoad: this.afterLoad,\n          scrollOverflow: true,\n          scrollBar: false,\n          menu: '#menu',\n          navigation: true,\n          anchors: ['Popular', 'New', 'Challenge'],\n          sectionsColor: ['#41b883', '#ff5f45', '#0798ec', '#fec401', '#1bcee6', '#ee1a59', '#2c3e4f', '#ba5be9', '#b4b8ab']\n        }\n      }\n    },\n    methods: {\n      afterLoad () {\n        console.log('After load')\n        // this.page = fullpage_api.setAllowScrolling(true);\n      },\n      addSection (e) {\n        e.preventDefault()\n        var newSectionNumber = document.querySelectorAll('.fp-section').length + 1\n        // creating the section div\n        var section = document.createElement('div')\n        section.className = 'section'\n        section.innerHTML = `<h3>Section ${newSectionNumber}</h3>`\n        // adding section\n        document.querySelector('#fullpage').appendChild(section)\n        // creating the section menu element\n        var sectionMenuItem = document.createElement('li')\n        sectionMenuItem.setAttribute('data-menuanchor', 'page' + newSectionNumber)\n        sectionMenuItem.innerHTML = `<a href=\"#page${newSectionNumber}\">Section${newSectionNumber}</a>`\n        // adding it to the sections menu\n        var sectionsMenuItems = document.querySelector('#menu')\n        sectionsMenuItems.appendChild(sectionMenuItem)\n        // adding anchor for the section\n        this.options.anchors.push(`page${newSectionNumber}`)\n        // we have to call `update` manually as DOM changes won't fire updates\n        // requires the use of the attribute ref=\"fullpage\" on the\n        // component element, in this case, <full-page>\n        // ideally, use an ID element for that element too\n        this.$refs.fullpage.build()\n      },\n      removeSection () {\n        var sections = document.querySelector('#fullpage').querySelectorAll('.fp-section')\n        var lastSection = sections[sections.length - 1]\n        // removing the last section\n        lastSection.parentNode.removeChild(lastSection)\n        // removing the last anchor\n        this.options.anchors.pop()\n        // removing the last item on the sections menu\n        var sectionsMenuItems = document.querySelectorAll('#menu li')\n        var lastItem = sectionsMenuItems[sectionsMenuItems.length - 1]\n        lastItem.parentNode.removeChild(lastItem)\n      },\n      toggleNavigation () {\n        this.options.navigation = !this.options.navigation\n      },\n      toggleScrollbar () {\n        console.log('Changing scrollbar...')\n        this.options.scrollBar = !this.options.scrollBar\n      }\n    }\n  }\n</script>\n\n<style>\n    ul {\n        list-style-type: none;\n        padding: 0;\n    }\n    li {\n        display: inline-block;\n        margin: 0 10px;\n    }\n    a {\n        color: #42b983;\n    }\n    * {\n      font-family: 'Source Sans Pro', sans-serif;\n      text-align: center;\n    }\n</style>\n"],"sourceRoot":"src"}]}